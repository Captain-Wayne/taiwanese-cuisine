{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "backend",
            "image": "waynecoding/multi-backend",
            "essential": true,
            "hostname": "backend"
        },
        {
            "name": "frontend",
            "image": "waynecoding/multi-frontend",
            "essential": true,
            "hostname": "frontend"
        },
        {
            "name": "nginx",
            "image": "waynecoding/multi-nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "frontend",
                "backend"
            ]
        }
    ]
}